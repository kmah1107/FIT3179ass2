{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "description": "A bar chart showing the US population distribution of age groups in 2000.",
  "height": {"step": 1000},
  "data": {
    "url": "https://raw.githubusercontent.com/kmah1107/FIT3179ass2/main/covid_19_india.csv"
  },
  "mark": "bar",
  "encoding": {
    "y": {"aggregate": "sum", "field": "Confirmed"},
    "x": {"field": "State/UnionTerritory", "title": "States"}
  },
  "config": {}
}